{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "indent": "off",
        "linebreak-style": 0,
        "no-multiple-empty-lines": [2, {"max": 99999, "maxEOF": 0}],
        "eol-last": 0,
    }
}